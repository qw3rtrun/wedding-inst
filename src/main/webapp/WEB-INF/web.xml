         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <welcome-file-list>
        <welcome-file>invite.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>Instafeed</servlet-name>
        <servlet-class>ru.lenaitrun.Instafeed</servlet-class>
